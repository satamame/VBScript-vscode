{
  "name": "vbscript-language-server-j",
  "description": "Implementation of a VBScript language server in node.",
  "version": "1.2.0",
  "author": "satamame",
  "license": "MIT",
  "engines": {
		"vscode": "^1.43.0"
	},
  "dependencies": {
    "vscode-languageserver": "^3.1.0",
    "@types/node": "^8.0.26"
  },
  "devDependencies": {
    "typescript": "^2.1.5"
  },
  "scripts": {
    "compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
    "watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc --watch -p ."
  }
}
